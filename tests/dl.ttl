@base <http://www.physical-chemistry.com/2016/5/untitled-ontology-21> .

<http://www.physical-chemistry.com/2016/5/untitled-ontology-21> rdf:type owl:Ontology .


#################################################################
#
#    Object Properties
#
#################################################################


###  http://www.physical-chemistry.com/ancestorOf

:ancestorOf rdf:type owl:ObjectProperty .



###  http://www.physical-chemistry.com/brotherOf

:brotherOf rdf:type owl:ObjectProperty .



###  http://www.physical-chemistry.com/childOf

:childOf rdf:type owl:ObjectProperty ;
         
         owl:inverseOf :parentOf ;
         
         owl:propertyDisjointWith :parentOf .



###  http://www.physical-chemistry.com/createdBy

:createdBy rdf:type owl:ObjectProperty ;
           
           owl:equivalentProperty :manufacturedBy .



###  http://www.physical-chemistry.com/manufacturedBy

:manufacturedBy rdf:type owl:ObjectProperty .



###  http://www.physical-chemistry.com/parentOf

:parentOf rdf:type owl:ObjectProperty ;
          
          rdfs:subPropertyOf :ancestorOf .



###  http://www.physical-chemistry.com/sonOf

:sonOf rdf:type owl:ObjectProperty ;
       
       rdfs:subPropertyOf :childOf ;
       
       rdfs:domain :Male ;
       
       rdfs:range :Parent .



###  http://www.physical-chemistry.com/uncleOf

:uncleOf rdf:type owl:ObjectProperty ;
         
         owl:propertyChainAxiom ( :brotherOf
                                  :parentOf
                                ) .





#################################################################
#
#    Classes
#
#################################################################


###  http://www.physical-chemistry.com/Child

:Child rdf:type owl:Class .



###  http://www.physical-chemistry.com/Father

:Father rdf:type owl:Class ;
        
        rdfs:subClassOf :Parent .



###  http://www.physical-chemistry.com/Female

:Female rdf:type owl:Class ;
        
        owl:disjointWith :Male .



###  http://www.physical-chemistry.com/Human

:Human rdf:type owl:Class ;
       
       owl:equivalentClass :Person .



###  http://www.physical-chemistry.com/Male

:Male rdf:type owl:Class .



###  http://www.physical-chemistry.com/Mother

:Mother rdf:type owl:Class ;
        
        owl:equivalentClass [ owl:intersectionOf ( :Female
                                                   :Parent
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
        
        rdfs:subClassOf :Parent .



###  http://www.physical-chemistry.com/Parent

:Parent rdf:type owl:Class ;
        
        owl:equivalentClass [ rdf:type owl:Class ;
                              owl:unionOf ( :Father
                                            :Mother
                                          )
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :parentOf ;
                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :Child
                            ] .



###  http://www.physical-chemistry.com/Person

:Person rdf:type owl:Class .





#################################################################
#
#    Individuals
#
#################################################################


###  http://www.physical-chemistry.com/John

:John rdf:type owl:NamedIndividual ,
               :Person ;
      
      owl:sameAs :Johnny .



###  http://www.physical-chemistry.com/Johnny

:Johnny rdf:type owl:NamedIndividual ,
                 :Person .



###  http://www.physical-chemistry.com/Julia

:Julia rdf:type owl:NamedIndividual ,
                :Person .





#################################################################
#
#    General axioms
#
#################################################################


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( :John
                        :Julia
                      )
] .




